    "react-testing-library": "^5.4.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "/tests/.*\\.(ts|tsx|js)$",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json"
    ],
    "verbose": true
  }
