{
  "name": "iowa-planeterrella",
  "version": "1.0.0",
  "description": "Example on using create-react-app with a Node Express Backend",	
  "license": "MIT",
  "dependencies": {
    "@firebase/app-types": "^0.3.3",
    "@firebase/util": "^0.2.6",
    "@types/jest": "^23.3.13",
    "@types/jsdom": "^12.2.1",
    "@types/node": "^10.12.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/reactstrap": "^7.1.1",
    "bootstrap": "^4.2.1",
    "firebase": "^5.8.2",
    "firebase-admin": "^7.0.0",
    "jquery": "^3.3.1",
    "popper.js": "1.14.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "reactstrap": "^7.1.0",
    "typescript": "^3.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.13",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "jest": ">=22 <24",
    "jest-cli": "^24.0.0",
    "raf": "^3.4.1",
    "react-testing-library": "^5.4.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/tests/test-setup.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/tests/**/*.test.{ts,tsx,js,jsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/test-preprocessor.js",
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!react-bootstrap-table/dist/react-bootstrap-table.min.css)"
    ],
    "verbose": true
  }
}
